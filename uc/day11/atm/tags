msg1	server.c	/^struct Msg msg1, msg2;$/;"	v	typeref:struct:Msg
msg2	server.c	/^struct Msg msg1, msg2;$/;"	v	typeref:struct:
msgid	service.h	/^  int   msgid;$/;"	m	struct:Msg	access:public
mtype	service.h	/^  long    mtype;$/;"	m	struct:Service	access:public
